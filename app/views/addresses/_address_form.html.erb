<%= form_for @address do |f| %>

  <% if @address.persisted? %>
    <%= "address ID: #{@address.id}" %>
  <% else %>
    <p>N/A</p>
  <% end %>
  <br>
  <%=f.label :street_address %>
  <%=f.text_field :street_address, class: "form-control" %><br>

  <%=f.label :secondary_address %>
  <%=f.text_field :secondary_address, class: "form-control" %><br>

 


  <%=f.submit %>

<% end %>